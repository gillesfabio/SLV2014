{
  "opts": {
    "destination": "./docs/",
    "recursive": true
  },
  "source": {
    "include": [
      "src"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "applicationName": "SLVM2014",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "SLVM2014",
      "description": "",
      "keyword": ""
    }
  }
}
